{
  "name": "hyperreact",
  "version": "0.0.3",
  "description": "Any resemblance to an animation library is purely coincidental",
  "main": "hyperreact.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "HYPERREACT=build ./node_modules/.bin/rollup -c && HYPERREACT=build webpack --display-modules --progress --colors",
    "dev": "./node_modules/.bin/rollup -c && webpack --display-modules --progress --colors",
    "test": "eslint source/*.js && eslint test/*.js --rule 'no-undef:0' && mocha --compilers js:babel-core/register --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KevinDoughty/hyperreact.git"
  },
  "keywords": [
    "hyper",
    "additive",
    "relative",
    "animation",
    "react"
  ],
  "author": "Kevin Doughty",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/KevinDoughty/hyperreact/issues"
  },
  "homepage": "https://github.com/KevinDoughty/hyperreact#readme",
  "dependencies": {
    "hyperact": "^0.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "rollup": "^0.39.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
